<aura:component implements="flexipage:availableForAllPageTypes,force:appHostable" controller="fetchPicklistOptsController">
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:handler event="aura:waiting" action="{!c.showSpinner}"/>
    <aura:handler event="aura:doneWaiting" action="{!c.hideSpinner}"/>
    <aura:attribute name="spinner" type="boolean" default="FALSE"/>
    <aura:attribute name="optionsList" type="List"/>
    <aura:attribute name="folderList" type="List"/>
    <aura:attribute name="groupNameList" type="List"/>
    <aura:attribute name="dashboardFolderList" type="List"/>
    <aura:attribute name="profileOptionList" type="List"/>
    <aura:attribute name="objInfo" type="String" />
    <aura:attribute name="objectPermissions" type="ObjectPermissions[]" />
    <aura:attribute name="worflows" type="ProfileLayoutCls.cls_records[]" />
    <aura:attribute name="reportData" type="ReportInformation[]"/>
    <aura:attribute name="dashboardData" type="Dashboard[]"/>
     <aura:attribute name="groupUserData" type="User[]"/>
    <aura:attribute name="validationRules" type="ProfileLayoutCls.cls_records[]" />
    <aura:attribute name="customButtons" type="WebLink[]" />
    <aura:attribute name="apexPages" type="List[]" />
    <aura:attribute name="selectedObjPerm" type="String" />
    <aura:attribute name="fieldPermissions" type="FieldPermissions[]" />
    <aura:attribute name="selectedfldPerm" type="String" />
    <aura:attribute name="selectedTable1" type="String"/>
    <aura:attribute name="isDisabled" type="boolean" default = "false"/>
    <aura:attribute name="selectedLayout" type="String"/>
    <aura:attribute name="selectedTable" type="String"/>
    <aura:attribute name="selectedFolder" type="String"/>
    <aura:attribute name="selectedGroup" type="String"/>
    <aura:attribute name="selectedDahboardFolder" type="String"/>
    <aura:attribute name="profileLayouts" type="ProfileLayoutCls.cls_records[]"/>
    <aura:attribute name="processBuilders" type="ProcessBuilderMetaData.cls_records[]"/>
    <aura:attribute name="selTabId" type="String" default="tab1" />
    <!--loading spinner start-->
    <aura:if isTrue="{!v.spinner}">
        <div aura:id="spinnerId" class="slds-spinner_container">
            <div class="slds-spinner--brand  slds-spinner slds-spinner--large slds-is-relative" role="alert">
                <span class="slds-assistive-text">Loading...</span>
                <div class="slds-spinner__dot-a"></div>
                <div class="slds-spinner__dot-b"></div>
            </div>
        </div>
    </aura:if>
    <!-- Loading spinner end-->   
    <lightning:tabset onselect="{!c.onTabSelected}" variant="default" selectedTabId="{!v.selTabId}">
        <lightning:tab aura:id = "tab1" tabindex="1" id="tab1" label="Object Permissions">
            <div class="slds-form-element">
                <lightning:layout horizontalAlign="center">
                    <lightning:layoutItem size="1">
