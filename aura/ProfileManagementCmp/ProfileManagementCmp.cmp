                                    <th scope="row"><div class="slds-truncate" title="{!objPermission.Id}">{!objPermission.Id}</div></th>
                                    <td><div class="slds-truncate" title="{!objPermission.Parent.Profile.Name}">{!objPermission.Parent.Profile.Name}</div></td>
                                    <td><div class="slds-truncate" title="{!objPermission.SobjectType}">{!objPermission.SobjectType}</div></td>
                                    <td><div class="slds-truncate" title="{!objPermission.PermissionsCreate}">{!objPermission.PermissionsCreate}</div></td>
                                    <td><div class="slds-truncate" title="{!objPermission.PermissionsDelete}">{!objPermission.PermissionsDelete}</div></td>
                                    <td><div class="slds-truncate" title="{!objPermission.PermissionsEdit}">{!objPermission.PermissionsEdit}</div></td>
                                    <td><div class="slds-truncate" title="{!objPermission.PermissionsModifyAllRecords}">{!objPermission.PermissionsModifyAllRecords}</div></td>
                                    <td><div class="slds-truncate" title="{!objPermission.PermissionsRead}">{!objPermission.PermissionsRead}</div></td>
                                    <td><div class="slds-truncate" title="{!objPermission.PermissionsViewAllRecords}">{!objPermission.PermissionsViewAllRecords}</div></td>
                                </tr>
                            </aura:iteration>
                        </tbody>
                    </table>
                </div>
            </div>
        </lightning:tab>
